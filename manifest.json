{
  "manifest_version": 2,
  "name": "Lectern Extension",
  "version": "0.1",

  "description": "Keep track of your online lectures",
  "icons": { 
    "16": "res/icon_16.png",
    "48": "res/icon_48.png",
    "128": "res/icon_128.png" },

  "browser_action": {
      "default_icon": {
        "16": "res/icon_16.png",
        "24": "res/icon_24.png",
        "32": "res/icon_32.png"
      }
  },

  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "res/icon_16.png",
      "32": "res/icon_32.png",
      "48": "res/icon_48.png",
      "128": "res/icon_128.png"
    }
  }
}